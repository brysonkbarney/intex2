@model User
@{
    ViewData["Title"] = "ADD DETAILS";
}
<br/><br/><br/> <br/>
<p>Thank you for logging in with Google! 
    Please fill out a few more account details that Google is unable to give us.
    Providing us with these details will allow us to give you better personal recommendations</p>
<div asp-validation-summary="All" class="text-danger"></div>
  
<form method="post" asp-controller="Admin" asp-action="AddGoogleDetails">
    <input hidden asp-for="NetUserId" class="form-control"/>
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="FirstName"></label>
        <input asp-for="FirstName" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="LastName"></label>
        <input asp-for="LastName" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="BirthDate"></label>
        <input asp-for="BirthDate" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="CountryOfResidence"></label>
        <input asp-for="CountryOfResidence" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="Gender"></label>
        <input asp-for="Gender" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="Age"></label>
        <input asp-for="Age" class="form-control"/>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>
    
    